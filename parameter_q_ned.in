subsection Standard method parameters
	subsection Mesh
  		set refinements = 6
	end
  
	subsection Control flow
		# compute flag
	  	set compute solution = true
	  
	  	# verbose
	  	set verbose = true
	  
	  	# what solver to use
	  	set use direct solver = false
	  
	  	# renumbering to reduce bandwidth
	  	set dof renumbering = true
  	end
  	
  	# name of outputfile without ending
  	set filename output = Q-NED-Std
end


subsection Multiscale method parameters
	subsection Mesh
  		set global refinements = 2
  		set local refinements = 3
	end
  
	subsection Control flow
		# compute flag
	  	set compute solution = false
	  
	  	# verbose
	  	set verbose = true
	  	set verbose basis = true
	  
	  	# what solver to use
	  	set use direct solver = false
	  	set use direct solver basis = false
	  
	  	# renumbering to reduce bandwidth
	  	set dof renumbering = true
	  	
	  	# write first basis
	  	set write first basis = true 
  	end
  	
  	# name of outputfile without ending
  	set filename output = Q-NED-Ms
end


subsection Equation parameters
	subsection Right-hand side
		set Function constants  = 
		
		# Separate vector valued expressions by ';' as ',' is used internally by the
		# function parser.
		set Function expression = 1; 1; 1
	
		# The name of the variables as they will be used in the function, separated
		# by ','.
		set Variable names      = x,y,z
	end
	
	subsection Diffusion A
		set frequency x = 0
		set frequency y = 0
		set frequency z = 0
		
		# scaling parameter
		set scale x = 1.0
		set scale y = 1.0
		set scale z = 1.0
		
		# Rotate with fixed Euler angles
		set rotate = false
	end
	
	subsection Diffusion B
		# Separate constants by comma ','
		set Function constants = k=2, scale=1
		
		# Separate vector valued expressions by ';' as ',' is used internally by the
		# function parser.
		set Function expression = scale * (1.0 - 0.99 * sin(2*pi*k*x))
		
		# The name of the variables as they will be used in the function, separated
		# by ','.
		set Variable names      = x,y,z,t
	end
	
	subsection Diffusion B inverse
		# Separate constants by comma ','
		set Function constants = k=2, scale=1
		
		# Separate vector valued expressions by ';' as ',' is used internally by the
		# function parser.
		set Function expression = 1 / (scale * (1.0 - 0.99 * sin(2*pi*k*x)))
		
		# The name of the variables as they will be used in the function, separated
		# by ','.
		set Variable names      = x,y,z,t
	end
end
